{{ "{% extends 'MvCrudGeneratorBundle::layout.html.twig' %}" }}

{{ "{% trans_default_domain 'mvcrud' %}" }}

{{ "{% block content -%}" }}
    <div class="admin-block">
        <h1>{{ "{{ '%entity% creation'|trans({'%entity%':'" ~ entity ~ "'})|capitalize }}" }}</h1>
        <div class="admin-block-content">

            <form action="{{ "{{ path('" ~ route_name_prefix ~ "_create') }}" }}" method="post" {{ "{{ form_enctype(form) }}" }}>
                {{ "{{ form_widget(form) }}" }}
                <p>
                    <button type="submit">{{ "{{ 'create'|trans|capitalize }}" }}</button>
                </p>
            </form>

            {% set hide_edit, hide_delete = true, true %}
            {% include 'crud/views/others/record_actions.html.twig.twig' %}
        </div>
    </div>
{{ "{% endblock %}" }}
